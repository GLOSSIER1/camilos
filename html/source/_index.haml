              Nam pharetra laoreet imperdiet volutpat etiam consequat feugiat.
          %hr/
          .5grid
            .row
              .6u
                / Contact Form
                %section.footer-one
                  %form{:action => "#", :method => "post"}
                    .5grid
                      .row
                        .6u
                          %input#contact-name.text{:name => "name", :placeholder => "Name", :type => "text"}/
                        .6u
                          %input#contact-email.text{:name => "name", :placeholder => "Email", :type => "text"}/
                      .row
                        .12u
                          %textarea#contact-message{:name => "message", :placeholder => "Message"}
                      .row
                        .12u
                          %ul.actions
                            %li
                              %input.button.button-style1{:type => "submit", :value => "Send"}/
